<section id= "banner-paper"> 


<style type="text/css">  img{
  -webkit-filter: sepia(70%);
  margin-right: auto;
  margin-left: auto;
  border-radius: 8px;
}</style>


<% @users.each do |u| %>


	<section id="portfolio" class="darker">
	<div class= "container">
	<div class="well text-center">
	Username: <%= u.username %><br>
	Email: <%= u.email %><br>
	<%= u.about %><br>
	
	<img src="/assets/<%= u.image %>" class="user-image" /img>
	<br>
	
	</div>
	</div>
<% end %>

</section>
</section>
